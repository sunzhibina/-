/*页面整体背景色*/
.allPageBGcolor {background: #f8f5f4;}
/*白色背景*/
.itemBackColor {background: #fff;}
.whiteColor {background: #fff;}
/*白色文字*/
.whiteFontColor {color: #fff;}
/*选中的颜色*/
.checkedColor {color: #e9302d;}
/*选中的背景颜色*/
.checkedBackColor {background: #e9302d;}
/*未选择块的背景颜色*/
.blockBackColor {background: #f7f7f7;}
/*第一类文字的字体大小-16*/
.oneFontSize {font-size: 4.27vw;}
/*第一类文字的折行宽度*/
.oneFontWidth {width: 79.2vw;}
/*第一类文字的颜色*/
.oneFontColor {color: #212121;}
/*第二类文字的颜色*/
.twoFontColor {color: #757575;}
/*第二类文字的大小-14*/
.twoFontSize {font-size: 3.73vw;}
/*第二类文字的高度*/
.twoHeight {height: 4.53vw;}
/*第三类文字的颜色*/
.threeFontColor {color: #9b9b9b;}
/*第三类文字的大小-12*/
.threeFontSize {font-size: 3.2vw;}
/*第四类文字的大小-18*/
.fourFontSize {font-size: 4.8vw;}
.fourFontColor {color: #4a4a4a;}
/*第四类文字的折行宽度*/
.fourFontWidth {width: 50.93vw;}
/*第五类文字的大小-10*/
.fiveFontSize {font-size: 2.67vw;}
/*第六类文字的大小-26*/
.sixFontSize {font-size: 6.93vw;}
/*第六类文字的大小-20*/
.sevenFontSize {font-size: 5.33vw;}
/*卡片文字整体上下间距*/
.textPadding {padding: 3.73vw 0;}
