.bodyPadding {padding: 0 5.33vw;}
/*卡片弧度*/
.cardBorder {border-radius: 2.67vw;margin: 2.13vw 0;}
/*搜索框边框式样设定*/
.searchBorder {border: 1px solid #ccc;border-radius: 4.8vw;}
/*搜索框*/
.searchView {position: relative;}
/*搜索框中的搜索图标*/
.searchView image {width: 3.47vw;height: 3.47vw;margin: 0 2.67vw 0 5.33vw;}
.publicSearchView image {width: 3.47vw;height: 3.47vw;margin: 0 2.67vw 0 5.33vw;}
/*搜索栏内*/
/*搜索的省份*/
.provinceView {width: 22.93vw;}
/*搜索的省份的下箭头图片*/
.provinceView image {width: 2.67vw;height: 1.6vw;margin: 0 0 0 1.07vw;}
/*搜索框+省份*/
.searchWrapper {padding: 2.13vw 0 0 0;}
/*通用搜索框*/
.publicSearchWrapper {padding: 2.13vw 0;position: relative;}
/*985、211类似院校标签的外层式样*/
.terraceBorder {margin-right: 1.07vw;box-sizing: border-box;text-align: center;border: 1px solid #f66666;border-radius: 1.07vw;}
/*右边向右的箭头图片大小*/
.rightIconWH {width: 1.87vw;height: 3.2vw;}
/*项目条的点击态*/
.itemHover {opacity: 0.8;}
/*文字指定宽度并显示省略号*/
.textOmit {overflow: hidden;white-space: nowrap;text-overflow: ellipsis;}
/*项目条下方的分割线*/
.bottomBorder {border-bottom: 1px solid #e9e9e9;}
/*大学详情页面的专业块边框*/
.disciplineBlock-border {border-radius: 3.47vw;border: 1px solid #ccc;}
.grayBorder{border: 1px solid #ccc;}
/*大学详情页面的专业块文字*/
.disciplineBlock-text {padding: 0 2.13vw;height: 6.67vw;}
/*文字折行*/
.textBreak {white-space: normal;word-wrap: break-word;}
/*44*/
.buttonHeight{height:11.73vw;}
/*54*/
.operationItemWH {height: 14.4vw;width: 100%;}
/*第一个按钮渐变色*/
.gradientOne {background-image: linear-gradient(137deg, #ffa5a6 0%, #fa9596 100%);}
.gradientTwo {background-image: linear-gradient(-52deg, #f84949 1%, #fa6565 100%);}
.gradientThree {background-image: linear-gradient(-52deg, #e9302d 1%, #ff5053 100%);}
.operationItemBtn {text-align: center;padding: 2.67vw 6.93vw;}
.relativeStyle {position: relative;}
.absoluteStyle {position: absolute;}
.fixedStyle {position: fixed;}
/*抽屉的下圆角*/
.drawerRadius {border-radius: 0 0 2.67vw 2.67vw;}
.itemListWidth {width: 83.73vw;}
.pageItemWidth {width: 91.47vw;margin: 0 auto;}
.flexjsbetween {justify-content: space-between;}
